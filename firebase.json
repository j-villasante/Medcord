{
  "hosting": {
    "public": "./",
    "ignore": [
      "**/node_modules/**",
      "**/src/**",
      "**/.*",
      "**/*.json",
      "**/*.config.js",
      "**/*.log",
      "**/*.md",
      "./dist/*.map",
      ".git/**"
    ],
    "rewrites": [
      {
        "source": "**",
        "destination": "/index.html"
      }
    ]
  }
}